<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bulma@0.9.2/css/bulma.min.css">
</head>
<body>
<%- include('partials/navbar') %>
<table class="table is-hoverable is-striped is-fullwidth">
    <thead>
    <tr>
        <th>Title</th>
        <th>Platform</th>
        <th>Date Added</th>
        <th>Cost</th>
        <th>Edition</th>
    </tr>
    </thead>
    <tbody>
    <% library.forEach(function(game) { %>
        <tr>
            <th><%= game.title %></th>
            <td><%= game.platform %></td>
            <td><%= game.dateAdded %></td>
            <td><%= game.cost %></td>
            <td><%= game.edition %></td>
        </tr>
    <% }); %>
    </tbody>
</table>
</body>
</html>
